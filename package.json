{
  "name": "elmTodoApp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:css": "postcss src/assets/css/main.css -o src/main.css",
    "watch:css": "postcss src/assets/css/main.css -o src/main.css -w",
    "build": "npm-run-all build:css build-js",
    "build-js": "node scripts/build.js",
    "start": "npm-run-all -p watch:css start-js",
    "start-js": "node scripts/start.js",
    "make": "elm make",
    "repl": "elm repl",
    "reactor": "elm reactor",
    "test": "elm-test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 9"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 9",
      "BlackBerry >= 7",
      "Chrome >= 9",
      "Firefox >= 4",
      "Explorer >= 9",
      "iOS >= 7",
      "Safari >= 6",
      "ExplorerMobile >= 9"
    ]
  },
